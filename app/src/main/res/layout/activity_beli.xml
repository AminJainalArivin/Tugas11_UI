<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:layout_width="410dp"
            android:layout_height="70dp"
            android:src="@drawable/groupl"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Pembelian"
            android:textSize="27dp"
            android:layout_gravity="center"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_31"
            android:layout_marginTop="20dp"
            android:id="@+id/ivQurban"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_32"
            android:layout_marginTop="10dp"
            android:id="@+id/ivVoucher"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_33"
            android:layout_marginTop="10dp"
            android:id="@+id/ivPLN"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_34"
            android:layout_marginTop="10dp"
            android:id="@+id/ivMoney"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_35"
            android:layout_marginTop="10dp"
            android:id="@+id/ivPaket"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_36"
            android:layout_marginTop="10dp"
            android:id="@+id/ivTopUp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_37"
            android:layout_marginTop="10dp"
            android:id="@+id/ivStreaming"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_38"
            android:layout_marginTop="10dp"
            android:id="@+id/ivAqiqah"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_39"
            android:layout_marginTop="10dp"
            android:id="@+id/ivSBN"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_40"
            android:layout_marginTop="10dp"
            android:id="@+id/ivOTP"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/group_41"
            android:layout_marginTop="10dp"
            android:id="@+id/ivGPlay"/>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="103dp"
            android:src="@drawable/group_55" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>